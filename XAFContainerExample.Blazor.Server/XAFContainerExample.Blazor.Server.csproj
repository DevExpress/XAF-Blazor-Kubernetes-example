<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Deterministic>false</Deterministic>
    <AssemblyVersion>1.0.*</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Configurations>Debug;Release;EasyTest</Configurations>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Model.xafml" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Model.xafml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
	  <PackageReference Include="DevExpress.ExpressApp.Blazor" Version="22.2.4" />
	  <PackageReference Include="DevExpress.ExpressApp.CodeAnalysis" Version="22.2.4" />
	  <PackageReference Include="DevExpress.ExpressApp.Dashboards.Blazor" Version="22.2.4" />
	  <PackageReference Include="DevExpress.ExpressApp.Office.Blazor" Version="22.2.4" />
	  <PackageReference Include="DevExpress.ExpressApp.ReportsV2.Blazor" Version="22.2.4" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.15.1" />
    <PackageReference Include="MySql.Data" Version="8.0.29" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\XAFContainerExample.Module\XAFContainerExample.Module.csproj" />
  </ItemGroup>
</Project>
